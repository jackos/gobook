{
	"name": "gobook",
	"publisher": "gobookdev",
	"displayName": "Go Notebook",
	"description": "Gobook: Run Go code interactively like a Jupyter Notebook. Works with .md and .gobook files",
	"version": "0.0.9",
	"preview": true,
	"bundledDependencies": [
		"gobookdev.gobook-kernel"
	],
	"enableProposedApi": true,
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.58.0"
	},
	"repository": {
		"url": "https://github.com/gobookdev/gobook"
	},
	"categories": [
		"Notebooks"
	],
	"activationEvents": [
		"onNotebook:markdown-notebook",
		"onLanguage:gobook",
		"onCommand:gobook.newNotebook"
	],
	"main": "./out/extension.js",
	"capabilities": {
		"virtualWorkspaces": true,
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {
		"commands": [
			{
				"title": "Gobook: Create a new blank Go Notebook",
				"command": "gobook.newGobook"
			}
		],
		"startEntries": [
			{
				"title": "Gobook",
				"description": "Run Go code interactively in a markdown format",
				"command": "gobook.newGobook"
			}
		],
		"notebooks": [
			{
				"type": "markdown-notebook",
				"displayName": "Gobook",
				"priority": "default",
				"selector": [
					{
						"filenamePattern": "*.{md,markdown,gobook}"
					}
				]
			}
		],
		"notebookRenderer": [
			{
				"id": "gobook",
				"displayName": "Gobook",
				"entrypoint": "./out/renderer/index.js",
				"mimeTypes": [
					"x-application/gobook"
				]
			}
		],
		"languages": [
			{
				"id": "gobook",
				"aliases": [
					"Gobook"
				],
				"filenamePatterns": [
					"*.gobook"
				]
			}
		],
		"grammars": [
			{
				"language": "gobook",
				"scopeName": "source.gobook",
				"path": "./syntaxes/gobook.tmGrammar.json",
				"embeddedLanguages": {
					"meta.embedded.block.go": "go"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"deploy": "npx vsce publish --noVerify",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}