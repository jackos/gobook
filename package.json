{
	"name": "gobook",
	"publisher": "gobookdev",
	"displayName": "Gobook Go Notebook",
	"description": "Run Go code in .md or .gobook files interactively like a Jupyter Notebook",
	"version": "0.0.1",
	"extensionDependencies": [
		"ethan-reesor.go-notebook-kernel"
	],
	"preview": true,
	"enableProposedApi": false,
	"engines": {
		"vscode": "^1.57.0"
	},
	"repository": {
		"url": "https://github.com/gobookdev/gobook"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onNotebook:markdown-notebook"
	],
	"main": "./out/extension.js",
	"capabilities": {
		"virtualWorkspaces": true,
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {
		"notebooks": [
			{
				"type": "markdown-notebook",
				"displayName": "Gobook Go Notebook",
				"priority": "default",
				"selector": [
					{
						"filenamePattern": "*.{md,markdown,gobook}"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"deploy": "npx vsce publish --noVerify",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}