{
	"name": "gobook",
	"publisher": "gobookdev",
	"displayName": "Go Notebook",
	"description": "Gobook: Run Go code interactively like a Jupyter Notebook. Works with .md and .gobook files",
	"version": "0.0.12",
	"preview": true,
	"enableProposedApi": true,
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.58.0"
	},
	"repository": {
		"url": "https://github.com/gobookdev/gobook"
	},
	"categories": [
		"Notebooks"
	],
	"activationEvents": [
		"onNotebook:gobook",
		"onLanguage:gobook",
		"onCommand:gobook.newNotebook"
	],
	"main": "./dist/extension.js",
	"capabilities": {
		"virtualWorkspaces": true,
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Go Notebook Kernel configuration",
			"properties": {
				"gobook.headers.input": {
					"description": "Header to place above each input cell when rendering markdown",
					"type": "string",
					"scope": "window",
					"default": "##### *in*"
				},
				"gobook.headers.output": {
					"description": "Header to place above each output cell when rendering markdown",
					"type": "string",
					"scope": "window",
					"default": "##### *out*"
				},
				"gobook.go.path": {
					"description": "Path to `go` executable",
					"type": "string",
					"scope": "window"
				},
				"gobook.kernel.path": {
					"description": "Path to kernel executable",
					"type": "string",
					"scope": "window"
				},
				"gobook.kernel.module": {
					"description": "Kernel module",
					"type": "string",
					"scope": "window"
				},
				"gobook.kernel.version": {
					"description": "Kernel version",
					"type": "string",
					"scope": "window"
				},
				"gobook.kernel.tags": {
					"description": "Tags to use when building the kernel",
					"type": "string",
					"scope": "window"
				}
			}
		},
		"commands": [
			{
				"command": "gobook.newGobook",
				"title": "Gobook: Create a new blank Go Notebook",
				"description": "Creates a blank notebook and opens it with Gobook"
			},
			{
				"command": "goNotebookKernel.kernel.restart",
				"title": "Go Notebook Kernel: Restart Kernel",
				"description": "Restart the running instance of the kernel"
			},
			{
				"command": "goNotebookKernel.kernel.stopSession",
				"title": "Go Notebook Kernel: Stop session",
				"description": "Stop the kernel session for the current document"
			},
			{
				"command": "goNotebookKernel.kernel.rebuild",
				"title": "Go Notebook Kernel: Rebuild Kernel",
				"description": "Rebuild the kernel"
			}
		],
		"startEntries": [
			{
				"title": "Gobook",
				"description": "Run Go code interactively in a markdown format",
				"command": "gobook.newGobook"
			}
		],
		"notebooks": [
			{
				"type": "gobook",
				"displayName": "Gobook",
				"priority": "default",
				"selector": [
					{
						"filenamePattern": "*.{md,markdown,gobook}"
					}
				]
			}
		],
		"notebookRenderer": [
			{
				"id": "gobook",
				"displayName": "Gobook",
				"entrypoint": "./out/renderer/index.js",
				"mimeTypes": [
					"x-application/gobook"
				]
			}
		],
		"languages": [
			{
				"id": "gobook",
				"aliases": [
					"Gobook"
				],
				"filenamePatterns": [
					"*.gobook"
				]
			}
		],
		"grammars": [
			{
				"language": "gobook",
				"scopeName": "source.gobook",
				"path": "./syntaxes/gobook.tmGrammar.json",
				"embeddedLanguages": {
					"meta.embedded.block.go": "go"
				}
			}
		]
	},
	"scripts": {
		"compile:extension": "yarn run bundle --sourcemap",
		"watch:extension": "yarn run bundle --sourcemap",
		"compile:grpc:js": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:src/proto --grpc_out=grpc_js:src/proto --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin kernel.proto",
		"compile:grpc:ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:src/proto kernel.proto",
		"compile:grpc": "yarn run compile:grpc:js && yarn run compile:grpc:ts",
		"bundle": "esbuild ./src/extension/main.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
		"watch": "yarn run watch:extension",
		"compile": "yarn run compile:extension && yarn run compile:grpc && yarn run bundle --minify",
		"vscode:prepublish": "yarn run compile"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.2.12",
		"google-protobuf": "^3.15.7"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@types/google-protobuf": "^3.7.4",
		"@types/node": "^14.14.37",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"vscode-test": "^1.3.0",
		"concurrently": "^6.0.0",
		"esbuild": "^0.12.9",
		"grpc": "^1.24.6",
		"grpc-tools": "^1.11.1",
		"grpc_tools_node_protoc_ts": "^5.3.0",
		"json": "^10.0.0",
		"ovsx": "^0.1.0-next.a9154dc",
		"typescript": "^4.2.3",
		"vsce": "^1.94.0"
	}
}